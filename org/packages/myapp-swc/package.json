{
  "name": "@org/myapp-swc",
  "version": "0.0.1",
  "dependencies": {
    "@swc/helpers": "~0.5.11"
  },
  "type": "commonjs",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "private": true,
  "nx": {
    "sourceRoot": "packages/myapp-swc/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/myapp-swc/dist",
          "main": "packages/myapp-swc/src/index.ts",
          "tsConfig": "packages/myapp-swc/tsconfig.lib.json"
        }
      }
    },
    "name": "myapp-swc"
  }
}
